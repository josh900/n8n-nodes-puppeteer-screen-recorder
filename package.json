    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "n8n-core": "~0.125.0",
    "n8n-workflow": "~0.107.0",
    "puppeteer": "^13.7.0",
    "puppeteer-screen-recorder": "^2.0.2"
