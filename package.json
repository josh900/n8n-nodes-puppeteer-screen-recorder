{
  "name": "n8n-nodes-puppeteer-screen-recorder",
  "version": "1.0.5",
  "description": "n8n node for recording websites using Puppeteer and puppeteer-screen-recorder",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
