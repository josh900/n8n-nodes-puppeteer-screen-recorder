{
  "name": "n8n-nodes-puppeteer-screen-recorder",
  "version": "1.0.0",
  "description": "Puppeteer Screen Recorder node for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Josh",
    "email": "murra178@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josh900/n8n-nodes-puppeteer-screen-recorder.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/PuppeteerScreenRecorder/PuppeteerScreenRecorder.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/puppeteer": "^7.0.4",
    "@types/request-promise-native": "~1.0.15",
    "gulp": "^4.0.2",
    "n8n-workflow": "^0.104.0",
    "prettier": "^2.5.1",
    "tslint": "^6.1.2",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "puppeteer": "^14.1.1",
    "puppeteer-screen-recorder": "^2.1.2"
  }
